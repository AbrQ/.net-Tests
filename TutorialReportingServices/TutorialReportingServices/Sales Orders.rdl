  <DataSets>
    <DataSet Name="AdventureWorksDataSet">
      <Query>
        <DataSourceName>AdventureWorksDW2012</DataSourceName>
        <CommandText>SELECT        DimCustomer.FirstName, DimCustomer.LastName, FactInternetSales.ProductKey, FactInternetSales.TotalProductCost
FROM            DimCustomer LEFT OUTER JOIN
                         FactInternetSales ON DimCustomer.CustomerKey = FactInternetSales.CustomerKey</CommandText>
      </Query>
